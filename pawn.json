{
    "user": "ClassyCountry",
    "repo": "sampvoice",
    "include_path": "documents",
    "resources": [
        {
            "name": "^sampvoice-(.*)-linux.tar.gz$",
            "platform": "linux",
            "archive": true,
            "includes": ["sampvoice-(.*)-linux"],
            "plugins": ["sampvoice-(.*)-linux/sampvoice.so"]
        },
        {
            "name": "^sampvoice-(.*)-win32.zip$",
            "platform": "windows",
            "archive": true,
            "includes": ["sampvoice-(.*)-win32"],
            "plugins": ["sampvoice-(.*)-win32/sampvoice.dll"]
        }
    ],
    "runtime": {
        "plugins": ["ClassyCountry/sampvoice"]
    }
}
